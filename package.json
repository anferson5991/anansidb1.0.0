{
  "name": "anansidb",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --win --x64 --ia32"
  },
  "homepage": "www.anansidb.com",
  "author": "anansidb",
  "license": "MIT",
  "keywords": [
    "electron",
    "scraper"
  ],
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.7",
    "sqlite3": "^5.1.6"
  },
  "build": {
    "appId": "com.anansidb.app",
    "productName": "AnansiDB",
    "directories": {
      "buildResources": "resources"
    },
    "win": {
      "icon": "resources/anansi.ico",
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "installerIcon": "resources/anansi.ico",
      "uninstallerIcon": "resources/anansi.ico",
      "allowToChangeInstallationDirectory": true
    },
    "extraResources": [
      {
        "from": "./resources/",
        "to": "./"
      }
    ]
    
  },
  "description": "AnansiDB - A data scraping company."
}
